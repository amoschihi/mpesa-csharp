{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Mpesa/**.cs"
          ],
          "src":"../"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "../docs/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default", "mathew/src"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "M-Pesa.Net",
      "_appLogoPath": "images/logo-small.png",
      "_appFaviconPath": "images/logo-small.png",
      "_enableSearch": true,
      "_appFooter": "Powered by DocFX | Copyright <a href=\"https://mureithi.me\" >Geoffrey Mureithi</a>"
    }
  },

}
